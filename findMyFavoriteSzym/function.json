    },
    {
      "name": "foodsiOutputblob",
      "type": "blob",
      "path": "frequency-of-packages/foodsi_storage.json",
      "connection": "DB_CONNECTION",
      "direction": "out"
    },
    {
      "type": "blob",
      "direction": "in",
      "name": "tgtgInputblob",
      "path": "frequency-of-packages/tgtg_storage.json",
      "connection": "DB_CONNECTION"
    },
    {
      "name": "tgtgOutputblob",
      "type": "blob",
      "path": "frequency-of-packages/tgtg_storage.json",
      "connection": "DB_CONNECTION",
      "direction": "out"
    }
  ]
